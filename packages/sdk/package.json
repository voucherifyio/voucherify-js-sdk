{
	"name": "@voucherify/sdk",
	"version": "2.9.4",
	"description": "Official Voucherify JS SDK",
	"author": "Voucherify",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/voucherifyio/voucherify-js-sdk.git",
		"directory": "packages/sdk"
	},
	"bugs": {
		"url": "https://github.com/voucherifyio/voucherify-js-sdk/issues",
		"email": "support@voucherify.io"
	},
	"keywords": [
		"Voucherify"
	],
	"sideEffects": false,
	"main": "dist/voucherifysdk.cjs",
	"module": "dist/voucherifysdk.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"README.md",
		"dist"
	],
	"scripts": {
		"start": "tsup --watch",
		"build": "tsup",
		"test": "jest",
		"lint": "yarn lint:ts && yarn lint:eslint",
		"lint:ts": "tsc --noEmit -p ./tsconfig.build.json",
		"lint:eslint": "eslint './src/**/*.{ts,tsx,js,d.ts}'",
		"lint:eslint:fix": "eslint './src/**/*.{ts,tsx,js,d.ts}' --fix",
		"docs": "typedoc",
		"prepublish": "yarn build"
	},
	"dependencies": {
		"axios": "1.12.2",
		"form-data": "4.0.4",
		"qs": "6.9.7"
	},
	"devDependencies": {
		"@types/jest": "30.0.0",
		"@types/qs": "6.9.7",
		"csv-stringify": "6.4.0",
		"dotenv": "16.3.1",
		"jest": "26.6.3",
		"jest-watch-typeahead": "3.0.1",
		"ts-jest": "26.4.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"browser": {
		"fs": false,
		"os": false,
		"path": false
	}
}
